{
  "name": "project-k",
  "version": "1.0.0",
  "description": "CAINZ Hackathon - Unified Development Platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
